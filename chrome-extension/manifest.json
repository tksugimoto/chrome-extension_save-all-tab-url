{
	"name": "Save opened windows/tabs",
	"version": "0.2.0",
	"manifest_version": 3,
	"minimum_chrome_version": "61",
	"description": "開いているWindow/Tab一覧を保存: Chromeを終了した時にWindow情報が保存されてないことがあるため",
	"permissions": [
		"alarms",
		"tabs",
		"storage"
	],
	"background": {
		"type": "module",
		"service_worker": "background.js"
	},
	"action": {
		"default_title": "開いているタブ一覧",
		"default_popup": "popup.html"
	}
}
